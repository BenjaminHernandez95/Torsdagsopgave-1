int input = -20;
int half = input/2;

if (input > 0){
while(input >= 0){

  
  if (input == 6){
   println("six");
 }
 
 else if (input == half){
   println("HALF!");
}

else{
 println(input);
 }
 
 
 input--;
}
}
else{

while (input <= 0){
 
 
 if (input == half){
   println("HALF!");
}

else{
 println(input);
 }
 
 
 input++;
}
}
